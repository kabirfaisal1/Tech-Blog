<div class="container" id="wrap">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <div class="column">
        <h2 class="title pb-4">create new account:</h2>
       
        <form class="signup-form">
          <div>
            <label for="username-signup">Full Name:</label>
            <input type="text" id="username-signup" />
          </div>
          <div>
            <label for="email-signup">Email</label>
            <input type="text" id="email-signup" />
          </div>
          <div>
            <label for="twitter-signup">Twitter</label>
            <input type="text" id="twitter-signup" />
          </div>
          <div>
            <label for="github-signup">Github</label>
            <input type="text" id="github-signup" />
          </div>
          <div>
            <label for="password-signup">Password</label>
            <input type="password" id="password-signup" />
          </div>
          <div>
            <button type="submit" class="button is-primary">Sign Up</button>
          </div>
        </form>
      </div>
    </div>
  </div>
</div>

<!-- javascript file-->
<script src="/js/signup.js"></script>